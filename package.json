{
    "name": "lavinia",
    "version": "2.8.0",
    "private": true,
    "description": "A seat distribution application for modern democracies",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "start": "webpack-dev-server --mode development --hot --progress --color --port 3000 --open",
        "start:debug": "webpack-dev-server --mode development --env.DEBUG true --hot --progress --color --port 3000 --open",
        "build": "webpack --env.NODE_ENV \"production\" -p --progress --colors",
        "build:dev": "webpack --env.NODE_ENV \"development\"",
        "format": "prettier --write \"src/**/*.{ts,tsx,css}\"",
        "precommit": "pretty-quick --staged && tslint -p .",
        "cy:verify": "cypress verify",
        "cy:run": "cypress run",
        "cy:test": "start-server-and-test start 3000 cy:run",
        "cy:ci:run": "cypress run --record --key $CYPRESS_RECORD_KEY --parallel --ci-build-id $BUILD_BUILDNUMBER",
        "cy:ci:test": "start-server-and-test start 3000 cy:ci:run"
    },
    "license": "MIT",
    "devDependencies": {
        "@babel/core": "^7.9.6",
        "@types/lodash": "^4.14.151",
        "@types/node": "10.3.2",
        "@types/react": "16.9.35",
        "@types/react-dom": "^16.9.8",
        "@types/react-redux": "^7.1.8",
        "@types/react-table": "^6.8.1",
        "@types/webpack": "4.41.13",
        "@types/webpack-env": "1.15.2",
        "babel-loader": "^8.1.0",
        "bulma": "^0.8.2",
        "bulma-pageloader": "^0.3.0",
        "bulma-switch": "^2.0.0",
        "bulma-tooltip": "^3.0.2",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^3.5.3",
        "cypress": "^4.8.0",
        "dotenv-webpack": "^1.8.0",
        "file-loader": "^6.0.0",
        "hard-source-webpack-plugin": "^0.13.1",
        "html-loader": "^1.1.0",
        "html-webpack-plugin": "^4.3.0",
        "husky": "^4.2.5",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "^4.14.1",
        "prettier": "^2.0.5",
        "pretty-quick": "^2.0.1",
        "react-hot-loader": "^4.12.21",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.2.1",
        "ts-loader": "^7.0.4",
        "tslint": "^6.1.2",
        "tslint-config-prettier": "^1.18.0",
        "tslint-react": "^5.0.0",
        "typescript": "^3.9.2",
